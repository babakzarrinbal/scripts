Windows Registry Editor Version 5.00

[HKEY_CLASSES_ROOT\.sh]
@="GitBashScript"

[HKEY_CLASSES_ROOT\GitBashScript]
@="Shell Script"

[HKEY_CLASSES_ROOT\GitBashScript\shell]
@=""

[HKEY_CLASSES_ROOT\GitBashScript\shell\open]

[HKEY_CLASSES_ROOT\GitBashScript\shell\open\command]
@="\"C:\\Program Files\\Git\\git-bash.exe\" -c \"bash '%1'; read -p 'Press any key to continue...'\""
